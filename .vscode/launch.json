            "type": "dart",
            "args": [
                "-d",
                "chrome"
            ]
        },
        {
            "name": "flutter_example app debug",
            "cwd": "flutter_example",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d"
            ]
        },
        {
            "name": "flutter_example (profile mode)",
            "cwd": "flutter_example",
            "request": "launch",
            "type": "dart",
